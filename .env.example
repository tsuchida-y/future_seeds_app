# 環境変数テンプレート
# このファイルをコピーして .env ファイルを作成してください
# .env ファイルは .gitignore に追加されています

# Android用APIキー
GOOGLE_MAPS_ANDROID_API_KEY=your_google_maps_api_key_here

# iOS用APIキー
GOOGLE_MAPS_IOS_API_KEY=your_google_maps_api_key_here

# Firebase Configuration (通常は google-services.json / GoogleService-Info.plist で管理)
# FIREBASE_PROJECT_ID=your_firebase_project_id

# PayPay寄付URL（実際のURLに置き換えてください）
PAYPAY_DONATION_URL=paypay://payment?link_id=FUTURE_SEEDS_DONATION

# クレジットカード寄付サイトURL
DONATION_WEBSITE_URL=https://future-seeds.example.com/donate

# その他の設定
# DEBUG_MODE=true
